{
  "name": "vue-file-agent",
  "version": "1.2.2",
  "description": "High performant Vue file upload component with elegant and distinguishable previews for every file type and support for drag and drop, validations, default uploader with progress support and externally customizable in the \"vue way\"",
  "main": "dist/vue-file-agent.umd.js",
  "types": "src/index.d.ts",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue-file-agent ./src/index.js",
    "watch": "vue-cli-service build --watch --target lib --name vue-file-agent ./src/index.js",
    "upload-server": "node upload-server-examples/node/upload-server.js",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/safrazik/vue-file-agent.git"
  },
  "keywords": [
    "vue",
    "file",
    "upload",
    "input",
    "progress",
    "drag",
    "drop"
  ],
  "author": "Safraz Razik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/safrazik/vue-file-agent/issues"
  },
  "homepage": "https://github.com/safrazik/vue-file-agent#readme",
  "files": [
    "/src",
    "/dist"
  ],
  "dependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
